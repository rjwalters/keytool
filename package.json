{
  "name": "keytool",
  "version": "0.0.1",
  "type": "module",
  "description": "Basic key shares tool",
  "main": "electron/main.js",
  "author": {
    "name": "Robb Walters",
    "url": "https://github.com/rjwalters"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjwalters/keytool.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production vite build && cross-env NODE_ENV=production electron .",
    "electron-dev": "NODE_ENV=development concurrently \"vite dev\" \"wait-on http://localhost:5173 && electron .\"",
    "make": "cross-env NODE_ENV=production npm run build && electron-builder build --mac --win --linux",
    "make-dir": "cross-env NODE_ENV=production npm run build && electron-builder build --mac --dir",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "lint-fix": "eslint . --fix",
    "ncu": "npx npm-check-updates -i",
    "test": "jest"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.8.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "electron-builder-squirrel-windows": "^25.1.8",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-svelte": "^3.2.8",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "svelte": "^5.2.7",
    "svelte-check": "^4.0.9",
    "tailwindcss": "^3.4.15",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11",
    "wait-on": "^8.0.1"
  },
  "dependencies": {
    "electron-serve": "^2.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "ethers": "^6.13.4"
  }
}
